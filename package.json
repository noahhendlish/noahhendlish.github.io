{
  "name": "noahhendlish.github.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "homepage":"https://github.com/noahhendlish/noahhendlish.github.io",

  "scripts": {
    "start": "npm-run-all --parallel watch server webpack",
    "build": "npm run css",
    "css-compile": "node-sass --include-path node_modules --output-style compressed --source-map true --source-map-contents true --precision 6 scss -o assets/css/",
    "css-lint": "stylelint scss/",
    "css-prefix": "postcss --replace assets/css/starter.css --use autoprefixer --map",
    "css-purge": "purgecss --keyframes --css assets/css/starter.css --content index.html \"node_modules/bootstrap/js/dist/{util,modal}.js\" --output assets/css/",
    "css": "npm-run-all css-compile css-prefix",
    "server": "serve --listen 3000",
    "watch": "nodemon -e html,scss -x \"npm run css\"",
    "webpack": "webpack --watch --mode=development",
    "test": "npm run css-lint && npm run css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noahhendlish/noahhendlish.github.io.git"
  },
  "keywords": [
    "bootstrap",
    "sass",
    "css",
    "javascript",
    "starter",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/noahhendlish/noahhendlish.github.io/issues"
  },
  "homepage": "https://github.com/noahhendlish/noahhendlish.github.io#readme",
  "dependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@craco/craco": "^6.3.0",
    "@formspree/react": "^2.2.4",
    "@octokit/core": "^3.5.1",
    "axios": "^0.22.0",
    "babel-loader": "^8.2.2",
    "bootstrap": "^5.1.1",
    "bootstrap-icons": "^1.5.0",
    "dotenv": "^10.0.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "webpack": "^5.56.1",
    "webpack-cli": "^4.8.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "purgecss": "^4.0.2",
    "serve": "^11.3.2",
    "stylelint": "^13.12.0",
    "stylelint-config-twbs-bootstrap": "^2.1.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.16"
  }
}
