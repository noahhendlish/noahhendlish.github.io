    "bootstrap": "^5.1.1",
    "bootstrap-icons": "^1.5.0",
    "dotenv": "^10.0.0",
    "formik": "^2.2.9",
    "jquery": "^3.6.0",
    "live-server": "^1.2.1",
    "material-components-web": "^6.0.0",
    "material-design-lite": "^1.3.0",
    "phantom": "^6.3.0",
    "popper.js": "^1.16.1",
